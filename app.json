{
  "expo": {
    "name": "SudaChat",
    "slug": "sudachat",
    "scheme": "sudachat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sudaChat.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/sudaChat.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sudaChat.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/images/sudaChat.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-secure-store",
      "expo-localization"
    ]
  }
}
